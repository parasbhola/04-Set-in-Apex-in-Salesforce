public class setDemo{
    public void setMethod(){
        
        Set<String> strset = new Set<String>();
       
        strset.add('India');
        
        System.debug('My country is ' +	 strset);                       
    }  
}

 //addAll(fromlist)  
public class setDemo{
    public void setMethod(){
    
        list<string> strlist = new list<string>	{'A','A'};
        Set<String> strset = new Set<String>();
       
        
        
        strset.add('India');
        strset.addall(strlist);
        
        System.debug('strlist =' + strlist);
        System.debug('strset  ' +	 strset);   

//clear  
public class setDemo{
    public void setMethod(){
    
        list<string> strlist = new list<string>	{'A','A'};
        Set<String> strset = new Set<String>();
       
        
        
        strset.add('India');
        strset.addall(strlist);
        
        System.debug('strlist =' + strset);
        strset.clear();
        System.debug('strset = ' +	 strset);         
        
    }
}
            
//clone
public class setDemo{
    public void setMethod(){
    
        list<string> strlist = new list<string>	{'A','A'};
        Set<String> strset = new Set<String>();
        Set<String> strset2 = new Set<String>();
       
        
        
        strset.add('India');
        strset.addall(strlist);
        
        System.debug('strlist =' + strset);
        strset2	=strset.clone();
        System.debug('strset = ' +	 strset);         
        
    }
}

//contain(setElement)
public class setDemo{
    public void setMethod(){
    
        list<string> strlist = new list<string>	{'A','A'};
        Set<String> strset = new Set<String>();
        Set<String> strset2 = new Set<String>();
       
        
         
        strset.add('India');
        strset.addall(strlist);
        
        System.debug(strset.contains('A'));
        strset2	=strset.clone();
        System.debug('strset2 = ' +	 strset2);         
        
    }
}

//contain(setElement)
public class setDemo{
    public void setMethod(){
    
        list<string> strlist = new list<string>	{'A','A'};
        Set<String> strset = new Set<String>();
        Set<String> strset2 = new Set<String>();
        Boolean B;
         
        
         
        strset.add('India');
        strset.addAll(strlist);
        b = strset.contains('B');
        
        
        System.debug('b =' +  b);
        System.debug(strset.contains ('A'));
        
    }
}

//Contains All(List to compare ) and containsAll (Set to compare)
//compare set with list and set with set
public class SetDemo {
    public void setMethod (){
        List<String> strlist = new List<String>{'A','B'};
            Set<String> strset = new Set <string>();
        Set<String> strset2 = new Set<string>();
         
        Boolean b; 
            
           strset.add('A');
        strset.add('B');
        
        b= strset.containsAll(strlist);
        system.debug('b='+ b);
           }
}


//equals 
public class SetDemo {
    public void setMethod (){
        List<String> strlist = new List<String>{'A','B'};
            Set<String> strset = new Set <string>();
        Set<String> strset2 = new Set<string>();
         
        Boolean b; 
            
           strset.add('A');
        strset.add('B');
        strset2.add('A');
        
        b= strset.equals	('strset2');
        system.debug('b='+ b);           
    }
}

//if else statement
public class SetDemo {
    public void setMethod (){
        List<String> strlist = new List<String>{'A','B'};
            Set<String> strset = new Set <string>();
        Set<String> strset2 = new Set<string>();
         
        Boolean b; 
            
           strset.add('A');
        strset.add('B');
        strset2.add('A');
        
        if(strset.isempty()){
            system.debug('Empty');
        }else{
            system.debug('Not empty');
        }
        }        


//remove set element
public class SetDemo {
    public void setMethod (){
        List<String> strlist = new List<String>{'A','B'};
            Set<String> strset = new Set <string>();
        Set<String> strset2 = new Set<string>();
         
        Boolean b; 
            
           strset.add('A');
        strset.add('B');   
         
        system.debug('strset' + strset 	);
        strset.remove('A');
        system.debug('strset' + strset);
    }
}

//remove All(List of Element to remove)
public class SetDemo {
    public void setMethod (){
        List<String> strlist = new List<String>{'A','B'};
            Set<String> strset = new Set <string>();
        Set<String> strset2 = new Set<string>();
         
        Boolean b; 
            
           strset.add('A');
        strset.add('B');   
        strset.add('C');
       strset.add('D');
       
         
        system.debug('strset' + strset 	);
        strset.removeAll(strlist);
        system.debug('strset' + strset);
    }
}


//retain All(List of Element to remove)
public class SetDemo {
    public void setMethod (){
        List<String> strlist = new List<String>{'A','B'};
            Set<String> strset = new Set <string>();
        Set<String> strset2 = new Set<string>();
         
        Boolean b; 
            
           strset.add('A');
        strset.add('B');   
        strset.add('C');
       strset.add('D');
       
         
        system.debug('strset' + strset 	);
        strset.retainAll(strlist);
        system.debug('strset' + strset);
    }
}

//size
public class SetDemo {
    public void setMethod (){
        List<String> strlist = new List<String>{'A','B'};
            Set<String> strset = new Set <string>();
        Set<String> strset2 = new Set<string>();
         
        Boolean b; 
            
           strset.add('A');
        strset.add('B');   
        strset.add('C');
       strset.add('D');
       
         
        system.debug('strset	size ' + strset.size() );
    
    }  
}
